<script>$(window).load(function () {
    var $container = $('.portfolioContainer');
    $container.isotope({
        filter: '.charteredaccountant',
        animationOptions: {
            duration: 780,
            easing: 'linear',
            queue: false } });



    $('.portfolioFilter a').click(function () {
        $('.portfolioFilter .current').removeClass('current');
        $(this).addClass('current');

        var selector = $(this).attr('data-filter');
        $container.isotope({
            filter: selector,
            animationOptions: {
                duration: 780,
                easing: 'linear',
                queue: false } });


        return false;
    });
});
